{{ header }}
<div id="product-category" class="container">
   <div class="row">
      {{ column_left }}
      {% if column_left and column_right %}
      {% set class = 'col-sm-6' %}
      {% elseif column_left or column_right %}
      {% set class = 'col-xs-12 col-sm-8 col-md-9 col-lg-9' %}
      {% else %}
      {% set class = 'col-sm-12' %}
      {% endif %}
      <div id="content" class="{{ class }}">
         {{ content_top }}
         <ul class="breadcrumb">
            {% for breadcrumb in breadcrumbs %}
            <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
            {% endfor %}
         </ul>
         <h1 class="co-heading">Thanks</h1>
         <div class="col-md-12">
            <div class="row">
             <div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> You have already place order this week!</div>
            </div>
         </div>
         {{ content_bottom }}
      </div>
      {{ column_right }}
   </div>
</div>

{{ footer }}