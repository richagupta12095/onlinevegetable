<script type="text/javascript"><!--
    $(function(){
        if(typeof localStorage === "undefined") return;

        $("a[data-toggle=tab]").click(function(){
            localStorage.setItem('myTab', $(this).attr('href'));
        });

        var myTab = localStorage.getItem('myTab');
        $('a[href=' + myTab + ']').click();


        //----
        $(".panel-heading a").click(function(){
            localStorage.setItem('myPanel', $(this).attr('href'));
        });


        var myPanel = localStorage.getItem('myPanel');
        $('a[href=' + myPanel + ']').click();

    });

//--></script>

<script>
    $(function(){
        // bind change event to select
        $('[dynamic_select]').on('change', function () {

            var storeUrl = $(this).attr('store_url');

            var storeId = $(this).val(); // get selected value
            if (storeId) { // require a URL
                window.location = storeUrl + '&store_id=' + storeId; // redirect
            }
            return false;
        });
    });
</script>

