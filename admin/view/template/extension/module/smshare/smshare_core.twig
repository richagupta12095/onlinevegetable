{{ header }}{{ column_left }}

<div id="content">
    <div class="page-header">
        <div class="container-fluid">

            {{ fragment_smshare_controls }}

            <h1>{{ heading_title }} 2.0.0</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>

    <div class="container-fluid">
        {% if error.error_warning %}
            <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error.error_warning }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %}

        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ heading_title }}</h3>
            </div>

            <div class="panel-body">

                <form action="{{ action }}" id="tv-module-form" method="post" enctype="multipart/form-data"
                      class="form-horizontal">


                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#tab-sending-methods" data-toggle="tab">Sending methods</a></li>
                        <li               ><a href="#tab-sms-templates"   data-toggle="tab">SMS Templates</a></li>
                        <li               ><a href="#tab-number-insight"  data-toggle="tab">Number insight</a></li>
                        <li               ><a href="#tab-troubleshooting" data-toggle="tab">Troubleshooting</a></li>
                    </ul>

                    <div class="tab-content">
                        {{ tab_sending_methods }}
                        {{ tab_sms_templates }}
                        {{ tab_number_insight }}
                        {{ tab_troubleshooting }}
                    </div>

                </form>

            </div><!-- .panel-body -->
        </div>
    </div><!-- .container-fluid -->


    <div>
        <div class="container-fluid">
            {{ fragment_smshare_controls }}

            <div style="display:table">

    <a href="http://www.twitter.com/prowebtec" style="display: table-cell; padding-right: 7px;" target="_blank">
        <img alt="Follow smshare on Twitter" src="data:image/png;base64,">
    </a>

    <div id="fb-root" style="display: table-cell;"></div>
    <script>
        $(window).load(function() {
            (function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.7&appId=1710485205937194";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        });
    </script>
    <div class="fb-like" data-href="https://www.facebook.com/prowebtec/" data-layout="standard" data-action="like" data-size="small" data-show-faces="false" data-share="false"></div>

    <div><small>Like &amp; Claim your <a href="https://www.facebook.com/prowebtec" target="_blank">discount coupon code</a></small></div>
</div>

        </div>
    </div>


</div><!-- #content -->

